{% if admin.canAccessObject('edit', object) and admin.hasRoute('edit') %}
    <li>
        <a class="sonata-action-element" href="{{ path('admin_app_test_edit', {'id': object.id}) }}">
            <i class="fa fa-edit" aria-hidden="true"></i>
            {{ 'action_edit_test'|trans({}, 'SonataAdminBundle') }}
        </a>
    </li>
{% endif %}